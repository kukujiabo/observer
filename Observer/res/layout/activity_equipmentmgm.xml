    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/ll2"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ll" >

        <ImageView
            android:id="@+id/iv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@drawable/denglushoujihao" />

        <ImageView
            android:id="@+id/set_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/wenduchuanganqi" />

        <TextView
            android:id="@+id/set_model"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/set_icon"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@+id/set_icon"
            android:textSize="10sp"
            android:textColor="@color/black"
            android:text="温度传感器" />

         <TextView
             android:id="@+id/set_state"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignBottom="@+id/set_icon"
             android:layout_marginLeft="20dp"
             android:layout_toRightOf="@+id/set_icon"
             android:text="正在运行"
             android:layout_marginBottom="10dp"
             android:textColor="#C3C3C3"
             android:textSize="10sp" />

         <ImageView
             android:id="@+id/set_switch"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_alignParentRight="true"
             android:layout_alignTop="@+id/set_model"
             android:layout_marginRight="30dp"
             android:layout_marginTop="5dp"
             android:background="@null" />
         
         
      		 <View
     		 android:id="@+id/v"
    	     android:layout_width="fill_parent"
     		 android:layout_height="2dp"
     		 android:layout_alignBottom="@+id/iv1"
     		 android:layout_alignParentLeft="true"
     		 android:background="#C3C3C3" />
       
       </RelativeLayout>

    <RelativeLayout
        android:id="@+id/ll3"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ll2"
         >

        <ImageView
            android:id="@+id/iv2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@drawable/denglushoujihao" />

        <ImageView
            android:id="@+id/set_icon_two"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/shiduchuanganqi" />

        <TextView
            android:id="@+id/set_model_two"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/set_icon_two"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:layout_toRightOf="@+id/set_icon_two"
            android:text="湿度传感器"
            android:textColor="@color/black"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/set_state_two"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/set_icon_two"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/set_icon_two"
            android:text="系统故障"
            android:textColor="#C3C3C3"
            android:textSize="10sp" />

        <ImageView
            android:id="@+id/set_switch_two"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/set_model_two"
            android:layout_marginRight="30dp"
            android:layout_marginTop="5dp"
            android:background="@null" />

        <View
            android:id="@+id/View01"
            android:layout_width="fill_parent"
            android:layout_height="2dp"
            android:layout_below="@+id/iv2"
            android:layout_alignParentLeft="true"
            android:background="#C3C3C3" />
    </RelativeLayout>
       
</RelativeLayout>